{
  "name": "cash-program-design-wizard-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodekruis/cash-program-design-wizard.git",
    "directory": "client"
  },
  "scripts": {
    "ng": "ng",
    "set-env-variables": "node ./_set-env-variables.js",
    "check:icons-in-use": "node ./_check-icons-in-use.js",
    "prebuild": "npm run set-env-variables",
    "build": "ng build",
    "build:production": "npm run build -- --configuration=production",
    "postbuild:production": "npm run check:icons-in-use",
    "lint": "npm run lint:syntax && ng lint",
    "lint:syntax": "prettier --check .",
    "fix:syntax": "npm run lint:syntax -- --write",
    "preserve-static": "cp www/index.html www/404.html",
    "serve-static": "http-server ./www/ -c-0 --port 8081 --proxy http://localhost:8081?",
    "start:production": "npm run build:production && npm run serve-static",
    "start": "ng serve",
    "pretest": "npm run lint",
    "test": "ng test --configuration=ci",
    "watch:test": "ng test"
  },
  "engines": {
    "node": "^16.x",
    "npm": "^8.x"
  },
  "dependencies": {
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/forms": "~13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/router": "~13.2.2",
    "@angular/service-worker": "~13.2.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@capacitor/app": "^1.1.0",
    "@capacitor/clipboard": "^1.0.7",
    "@capacitor/core": "3.4.1",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/keyboard": "^1.2.1",
    "@capacitor/status-bar": "^1.0.7",
    "@ionic/angular": "6.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "file-saver": "^2.0.5",
    "ngx-markdown": "^13.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@angular/language-service": "~13.2.2",
    "@capacitor/cli": "3.4.1",
    "@ionic/angular-toolkit": "^6.0.0",
    "@ionic/cli": "^6.18.1",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.23",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "http-server": "^14.1.0",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.15",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "~4.5.5"
  }
}
